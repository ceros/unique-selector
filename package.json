{
  "name": "unique-selector",
  "version": "0.4.1",
  "description": "Given a DOM node, return a unique CSS selector matching only that element",
  "main": "./lib/index.js",
  "jsnext:main": "./src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run compile",
    "test": "npm run compile && mocha --ignore-leaks --require @babel/register",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "prepare": "npm run compile",
    "watch": "npm-scripts-watcher"
  },
  "watch": {
    "src/**/*.js": [
      "compile"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ericclemmons/unique-selector.git"
  },
  "keywords": [
    "dom",
    "html",
    "css",
    "selector",
    "event"
  ],
  "author": "Eric Clemmons <eric@smarterspam.com>",
  "contributors": [
    {
      "name": "Avraam Mavridis",
      "email": "avr.mav@gmail.com"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "chai": "^4.2.0",
    "component": "~0.10.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-lite": "^1.0.4",
    "eslint-plugin-react": "^7.15.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jquery": "^3.4.1",
    "mocha": "~6.2.1",
    "mocha-jsdom": "^2.0.0",
    "mocha-phantomjs": "~4.1.0"
  },
  "dependencies": {
    "cssesc": "^3.0.0"
  }
}
